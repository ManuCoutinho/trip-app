{
  "name": "trip-app",
  "version": "1.0.0",
  "private": true,
  "author": "Emanuela Coutinho",
  "license": "MIT License",
  "description": "places to travel and beautiful photos",
  "bugs": {
    "url": "https://github.com/ManuCoutinho/trip-app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuCoutinho/trip-app"
  },
  "keywords": [
    "next.js",
    "react",
    "typescript",
    "jest",
    "graphql"
  ],
  "engines": {
    "node": "16.15.1",
    "npm": ">=8.11"
  },
  "overrides": {
    "react": "18.2.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "prepare": "husky install",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --max-workers=25%",
    "coverage": "jest --coverage --silent",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src --fix",
      "jest --passWithNoTests --findRelatedTests"
    ],
    "*.{json,md, yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@rollbar/react": "^0.11.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "leaflet": "^1.9.2",
    "next": "^13.0.1",
    "next-seo": "^5.14.1",
    "nookies": "^2.5.2",
    "nprogress": "^0.2.0",
    "phosphor-react": "^1.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "^4.1.0",
    "rollbar": "^2.26.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/config-conventional": "^17.2.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "2.13.12",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@graphql-codegen/typescript-resolvers": "^2.7.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/leaflet": "^1.9.0",
    "@types/node": "18.8.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "next-router-mock": "^0.9.1",
    "prettier": "^2.7.1",
    "stylelint": "^15.6.2",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  }
}
